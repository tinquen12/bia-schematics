{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsTableComponent",
  "title": "Table Component",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What is the singular name of the entity you want to create the table ?"
    }
  },
  "required": ["name"]
}
